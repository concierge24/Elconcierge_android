<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/thumb_background">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/colorDarkPurple">
            </solid>
            <corners
                android:radius="10dp">
            </corners>
        </shape>
    </item>
    <item
        android:left="20dp"
        android:right="20dp"
        android:bottom="18dp"
        android:top="18dp"
        android:gravity="center"
        android:id="@+id/thumb_image"
        android:drawable="@drawable/ic_slide_arrow" />
</layer-list>
